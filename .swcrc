{
  "$schema": "https://json.schemastore.org/swcrc.json",
  "minify": true,
  "jsc": {
    "externalHelpers": true,
    "target": "es2015",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "pragma": "jsx",
        "importSource": "import {jsx} from '@emotion/react'"
      }
    }
  }
}
