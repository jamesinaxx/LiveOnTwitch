{
  "$schema": "https://json.schemastore.org/swcrc.json",
  "minify": true,
  "jsc": {
    "minify": {
      "compress": {
        "unused": true,
        "drop_console": true
      }
    },
    "transform": {
      "react": {
        "pragma": "jsx"
      }
    },
    "externalHelpers": true,
    "target": "es2015",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    }
  }
}
