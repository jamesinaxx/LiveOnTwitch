{
  "author": "Juliette Cordor",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "See who is live"
  },
  "background": { "scripts": ["SERVICE_WORKER"] },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage", "alarms"],
  "content_scripts": [
    {
      "matches": ["*://nowlive.jamesinaxx.me/auth/callback"],
      "js": ["CONTENT_SCRIPT"]
    }
  ]
}
