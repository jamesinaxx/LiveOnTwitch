{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "strict": true,
    "typeRoots": ["./src/types"],
    "types": ["vite/client"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "baseUrl": ".",
    "module": "ESNext",
    "target": "es2016",
    "incremental": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["dist", "node_modules"]
}
