{
  "name": "nowlive",
  "displayName": "Now Live",
  "description": "Instantly check which of your favorite Twitch streamers are live right from your favorite browser",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jamesinaxx/NowLive",
  "author": "jamesinaxx",
  "license": "MIT",
  "scripts": {
    "pre-commit": "yarn lint && yarn fmt",
    "dev": "webpack --mode development",
    "build": "rimraf dist && webpack --mode production",
    "build:analyze": "yarn build --analyze",
    "fmt": "prettier --write ./src/*/**.{js,jsx,ts,tsx,scss,css,sass} --ignore-path .gitignore",
    "fmt:check": "prettier --check ./src/*/**.{js,jsx,ts,tsx,scss,css,sass} --ignore-path .gitignore",
    "lint": "eslint ./src/*/**.{js,jsx,ts,tsx} --ignore-path .gitignore",
    "lint:fix": "eslint ./src/*/**.{js,jsx,ts,tsx} --ignore-path .gitignore --fix",
    "postinstall": "husky install"
  },
  "dependencies": {
    "fast-average-color": "6.4.2",
    "preact": "10.5.14",
    "react-icons": "4.2.0",
    "styled-components": "5.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@types/chrome": "0.0.157",
    "@types/dotenv-webpack": "7.0.3",
    "@types/node": "16.9.1",
    "@types/react": "17.0.20",
    "@types/rimraf": "3.0.2",
    "@types/sharp": "0.29.1",
    "@types/styled-components": "5.1.14",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-loader": "8.2.2",
    "dotenv-webpack": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "3.0.1",
    "hoist-non-react-statics": "3.3.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "rimraf": "3.0.2",
    "scheduler": "0.20.2",
    "sharp": "0.29.1",
    "supports-color": "9.0.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.2",
    "webpack": "5.52.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.8.0",
    "webpack-merge": "5.8.0"
  },
  "packageManager": "yarn@3.0.2"
}
