{
  "name": "nowlive",
  "displayName": "Now Live",
  "version": "1.0.0",
  "private": true,
  "description": "Instantly check which of your favorite Twitch streamers are live right from your favorite browser",
  "repository": "https://github.com/jewlexx/NowLive",
  "license": "MIT",
  "author": "jewlexx",
  "scripts": {
    "build": "parcel build manifest.json --config @parcel/config-webextension",
    "build:analyze": "yarn build --analyze",
    "dev": "webpack --mode development",
    "fmt": "prettier --write ./src/*/**.{ts,tsx} --ignore-path .gitignore",
    "fmt:check": "prettier --check ./src/*/**.{ts,tsx} --ignore-path .gitignore",
    "icons": "node scripts/icons.js",
    "lint": "eslint ./src/*/**.{ts,tsx} --ignore-path .gitignore",
    "lint:fix": "eslint ./src/*/**.{ts,tsx} --ignore-path .gitignore --fix"
  },
  "dependencies": {
    "fast-average-color": "9.1.1",
    "framer-motion": "6.3.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.4.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.5",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@parcel/config-webextension": "2.6.2",
    "@swc/core": "1.2.205",
    "@swc/helpers": "0.4.2",
    "@types/chrome": "0.0.191",
    "@types/dotenv-webpack": "7.0.3",
    "@types/node": "18.0.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/sharp": "0.30.4",
    "@types/styled-components": "5.1.25",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "browserslist": "4.21.0",
    "dotenv-webpack": "7.1.1",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "3.2.0",
    "html-webpack-plugin": "5.5.0",
    "install-peers": "1.0.4",
    "parcel": "2.6.2",
    "prettier": "2.7.1",
    "process": "0.11.10",
    "sharp": "0.30.7",
    "swc-loader": "0.2.3",
    "ts-node": "10.8.1",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0"
  },
  "packageManager": "yarn@3.2.0"
}
